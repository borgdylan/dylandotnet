@DEBUG_TRUE@	echo '#define DEBUG' | tee -a cflags.dyl
@NET45_FALSE@	echo '#undef NET_4_5' | tee -a cflags.dyl
@RX_TRUE@	echo '#define RX' | tee -a cflags.dyl

