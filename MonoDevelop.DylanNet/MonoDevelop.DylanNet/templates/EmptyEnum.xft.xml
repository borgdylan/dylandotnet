	<TemplateConfiguration>
		<_Name>Empty Enum</_Name>
		<Icon>md-enum-file</Icon>
		<_Category>General</_Category>
		<LanguageName>dylan.NET</LanguageName>
		<_Description>Creates an empty dylan.NET enum.</_Description>
