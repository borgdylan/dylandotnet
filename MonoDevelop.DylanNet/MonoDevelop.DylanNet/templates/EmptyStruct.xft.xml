<?xml version="1.0"?>
<Template Originator="Dylan Borg" Language="dylan.NET" Created="24/11/2013" LastModified="24/11/2013">
	
	<TemplateConfiguration>
		<_Name>Empty Struct</_Name>
		<Icon>md-struct-file</Icon>
		<_Category>General</_Category>
		<LanguageName>dylan.NET</LanguageName>
		<_Description>Creates an empty dylan.NET struct.</_Description>
		<DefaultFilename>EmptyStruct</DefaultFilename>
	</TemplateConfiguration>
	
	<TemplateFiles>
		<File DefaultExtension=".dyl" DefaultName="EmptyStruct" AddStandardHeader="True">
		<![CDATA[namespace ${Namespace}

	struct public auto ansi ${Name}
		
	end struct
	
end namespace]]>
		</File>
	</TemplateFiles>
	
	<FileOptions/>
	
</Template>
